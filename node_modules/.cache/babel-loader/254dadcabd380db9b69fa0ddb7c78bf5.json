   </div>\n    <md-field class=\"inputfield\">\n      <md-input\n        class=\"inputText\"\n        v-model=\"currentTodo\"\n        @keydown.enter=\"addTodo()\"\n        placeholder=\"Add a todo\"\n      >\n      </md-input>\n    </md-field>\n    <ul class=\"todos\">\n      <li\n        v-for=\"todo in todos\"\n        :key=\"todo.id\"\n        :class=\"{ editing: todo == editedTodo }\"\n      >\n        <!-- {{ todo.label }} -->\n        <p\n          class=\"title\"\n          contenteditable=\"true\"\n          v-on:keydown.enter=\"editedTodo($event, todo)\"\n          v-on:blur=\"editedTodo($event, todo)\"\n          v-bind:class=\"{ completed: todo.completed }\"\n        >\n          {{ todo.label }}\n        </p>\n        <input\n          type=\"checkbox\"\n          v-on:change=\"completedTodo(todo)\"\n          v-bind:checked=\"todo.completed\"\n        />\n        <button class=\"deletebutton\" @click=\"removeTodo(todo)\">\n          Delete Task\n        </button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      todos: [],\n      currentTodo: \"\",\n    };\n  },\n  methods: {\n    addTodo() {\n      this.todos.push({\n        id: this.todos.length,\n        label: this.currentTodo,\n        editedToDoId: false,\n        completed: false,\n      });\n      this.currentTodo = \"\";\n    },\n    removeTodo(todo) {\n      var index = this.todos.indexOf(todo);\n      this.todos.splice(index, 1);\n    },\n    completedTodo(todo) {\n      todo.completed = !todo.completed;\n    },\n    editToDo(todo) {\n      return \"<input>\" + todo.label + \"</input>\";\n      // todo.editedToDoId = true;\n      // placeholder=todo.label\n    },\n  },\n};\n</script>\n\n<style>\n.title1 {\n  color: red;\n  padding: 50px;\n  margin: auto;\n  text-align: center;\n}\n.mainDiv {\n  background-color: black;\n}\n.inputfield {\n  left: 50px;\n}\n.inputText {\n  color: gray;\n}\n.completed {\n  text-decoration: line-through;\n}\n.not-completed {\n  color: black;\n}\n.deletebutton {\n  border: none;\n  color: orange;\n  background-color: black;\n  position: relative;\n  left: 100px;\n  cursor: pointer;\n}\n.editing {\n  display: block;\n}\n.todos {\n  color: blue;\n  list-style-type: none;\n}\ndiv {\n  background-color: black;\n  position: center;\n}\nbody {\n  background-color: black;\n}\n</style>\n"],"sourceRoot":"src"}]}